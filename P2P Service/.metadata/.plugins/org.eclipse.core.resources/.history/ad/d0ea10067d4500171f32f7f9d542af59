package Multicast;

import java.net.*;
import java.nio.*;

public class Server {

	public MulticastSocket ms;
	public InetAddress group;
	public String address;
	public byte [ ] b;
	public int ports;
	public int portc;
	
	public Server ( String address, int ports, int portc, InetAddress group ) {
		
		this.address = address;
		this.ports = ports;
		this.portc = portc;
		this.group = group;
		
	} // End constructor.
	
} // End class.
